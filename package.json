{
  "name": "WifiUC",
  "version": "1.3.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "deploy": "scripts/deploy.sh",
    "icon": "python scripts/icon.py",
    "postversion": "react-native-version",
    "bump_build": "react-native-version -b; git commit -am \"Bump build\"",
    "ios": "scripts/run_ios.sh",
    "android": "scripts/run_android.sh",
    "both": "npm run android &>logs/run_android.log & npm run ios &>logs/run_ios.log & echo 'Just wait'",
    "fastlane": "cd ios; fastlane; cd ../android; fastlane",
    "metadata": "cd ios; fastlane metadata; cd ../android; fastlane metadata",
    "distribute": "cd ios; fastlane distribute; cd ../android; fastlane distribute",
    "build": "cd ios; fastlane build; cd ../android; fastlane build",
    "generate_env": "./scripts/generate_env.sh"
  },
  "dependencies": {
    "buffer": "^5.0.2",
    "cheerio": "^0.22.0",
    "events": "^1.1.1",
    "react": "~15.4.0",
    "react-native": "0.41.2",
    "react-native-dotenv": "0.0.3",
    "react-native-keychain": "^1.1.0",
    "react-native-storage": "^0.1.4",
    "react-native-vector-icons": "^4.0.0",
    "react-navigation": "^1.0.0-beta.1",
    "stream": "0.0.2",
    "tcomb-form-native": "^0.6.4"
  },
  "devDependencies": {
    "babel-jest": "18.0.0",
    "babel-preset-react-native": "1.9.1",
    "jest": "18.1.0",
    "react-test-renderer": "~15.4.0"
  },
  "jest": {
    "haste": {
      "defaultPlatform": "ios",
      "platforms": ["android", "ios", "native"],
      "providesModuleNodeModules": [
        "react-native"
      ]
    },
    "moduleNameMapper": {
      "^[./a-zA-Z0-9$_-]+\\.(bmp|gif|jpg|jpeg|png|psd|svg|webp)$": "RelativeImageStub",
      "^React$": "./node_modules/react"
    },
    "modulePathIgnorePatterns": [
      "./node_modules/react-native/Libraries/react-native/",
      "./node_modules/react-native/packager/"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|react-clone-referenced-element|tcomb-form-native|react-native-keychain)"
    ],
    "setupFiles": [
      "./node_modules/react-native/jest/setup.js"
    ],
    "testEnvironment": "node",
    "preset": "react-native"
  }
}
