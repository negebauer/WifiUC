{
  "name": "WifiUC",
  "version": "1.2.0",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "deploy": "scripts/deploy.sh",
    "icon": "python scripts/icon.py",
    "postversion": "react-native-version",
    "build": "react-native-version -b; git commit -am \"Bump build\"",
