  },
  "dependencies": {
    "buffer": "^5.0.2",
    "cheerio": "^0.22.0",
    "events": "^1.1.1",
    "react": "~15.4.0-rc.4",
    "react-native": "0.40.0",
    "react-native-keychain": "^1.0.0",
    "react-native-storage": "^0.1.4",
    "react-native-vector-icons": "^4.0.0",
    "react-navigation": "^1.0.0-beta.1",
    "stream": "0.0.2",
    "tcomb-form-native": "^0.6.4"
  },
  "devDependencies": {
    "babel-jest": "18.0.0",
    "babel-preset-react-native": "1.9.1",
    "jest": "18.1.0",
    "react-test-renderer": "~15.4.0-rc.4"
  },
  "jest": {
    "preset": "react-native"
  }
